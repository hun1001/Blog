		"mdsvex": "^0.11.0",
		"mdsvex-relative-images": "^1.0.3",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"sass": "^1.76.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.4",
		"typescript": "^5.0.0",
